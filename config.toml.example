# OpenAI API 配置
[api.openai]
# OpenAI API 密钥，从 OpenAI 官网获取
api_key = "your-openai-api-key"
# API 基础 URL，默认为 OpenAI 官方地址
base_url = "https://api.openai.com/v1"
# 使用的模型名称
model = "gpt-3.5-turbo"
# 控制随机性，范围 0.0 到 1.0
temperature = 0.7
# 生成的最大令牌数
max_tokens = 2000
# 请求超时时间（秒）
timeout = 30
# 是否启用流式响应
stream = false
# 请求失败重试次数
retry_count = 3
# 重试延迟时间（秒）
retry_delay = 1

# SiliconFlow API 配置
[api.siliconflow]
# SiliconFlow API 密钥，从 SiliconFlow 官网获取
api_key = "your-siliconflow-api-key"
# API 基础 URL，默认为 OpenAI 官方地址
base_url = "https://api.siliconflow.cn/v1"
# 使用的模型名称
model = "Qwen/Qwen2.5-7B-Instruct"
# 控制随机性，范围 0.0 到 1.0
temperature = 0.7
# 生成的最大令牌数
max_tokens = 2000
# 请求超时时间（秒）
timeout = 30
# 是否启用流式响应
stream = false
# 请求失败重试次数
retry_count = 3
# 重试延迟时间（秒）
retry_delay = 1

# 日志配置
[logging]
# 日志级别：DEBUG, INFO, WARNING, ERROR, CRITICAL
level = "INFO"
# 日志消息格式
format = "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
# 日志文件路径
file = "app.log"
# 日志文件最大大小（字节），默认 10MB
max_size = 10485760
# 保留的日志文件数量
backup_count = 5 